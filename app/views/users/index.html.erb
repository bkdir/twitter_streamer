<% provide(:title, "Authenticated Users") %>
<h1>Authenticated Users</h1>
<ul>
  <% @users.each do |user| %>
    <li>Name: <%= user.name %></li>
    <li>Email: <%= user.email %></li>
    <li>Creation Date: <%= user.created_at %></li>
    <li>Last Updated: <%= user.updated_at %></li>
  <% end %>
</ul>
